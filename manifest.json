{
  "manifest_version": 2,
  "name": "TTV Bots Autocompletion",
  "version": "1.2.0",
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "options_ui": {
    "page": "html/options.html",
    "chrome_style": true
  },
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://twitch.tv/*",
        "*://go.twitch.tv/*",
        "*://www.twitch.tv/*"
      ],
      "css": ["css/style.css"],
      "js": [
        "js/prefixes.js",
        "js/utils.js",
        "js/enums.js",
        "js/utils.js",
        "js/infinite.js",
        "js/script.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Import commands from this file"
  },
  "permissions": [
    "https://ttv-bots.nastekali.party/",
    "storage", "unlimitedStorage",
    "activeTab"
  ]
}